<div data-controller="ar"></div>
<a-scene embedded arjs>
  <a-assets>
    <video id="video" loop="true" src="<%= cl_video_path(@scene.video.key) %>" crossorigin webkit-playsinline playsinline controls></video>
  </a-assets>
  <a-marker preset="hiro">
    <a-video src="#video" width="16" height="9" position="0 0 -20"></a-video>
    <a-entity camera></a-entity>
  </a-marker>
</a-scene>
